            {/block:PostNotes}
            </div>
            {/block:Date} 
            {/block:PermalinkPage}  
            
            {/block:Posts}
        </div><!-- end #content --> 
        
        {Block:IfNotInfiniteScroll}
            {block:Pagination}
            <div id="pagination">
            {block:PreviousPage}
                <a href="{PreviousPage}" class="prev">Previous</a>
            {/block:PreviousPage}

            {block:NextPage}
                <a href="{NextPage}" class="next">Next</a>
            {/block:NextPage}
            
            <div id="jump">
            {block:JumpPagination length="3"}
                {block:CurrentPage}
                    <span class="current_page">{PageNumber}</span>
                {/block:CurrentPage}

                {block:JumpPage}
                    <a class="jump_page" href="{URL}">{PageNumber}</a>
                {/block:JumpPage}
            {/block:JumpPagination}
            </div>
            
            <div class="clear"></div>
            </div>
            {/block:Pagination}
        {/Block:IfNotInfiniteScroll}
        
        <div class="clear"></div>
        </div><!-- end #container -->
    </div><!-- end #center -->

<script type="text/javascript" language="javascript" src="http://dl.dropbox.com/u/71900253/Themes/jquery_cc.js"></script>
<script type="text/javascript" src="http://static.tumblr.com/8jq17g7/iuqmdg3nw/stickyscroll.js"></script>
<script type="text/javascript">
    $(window).load(function(){
    var $content = $('#content');
    {block:IndexPage}
    $content.masonry({
      itemSelector: '#post, #pagination',
      columnWidth: 1
    });
    
    {block:IndexPage}
    {Block:IfInfiniteScroll}
    $content.infinitescroll({
        navSelector  : ".nav",
        nextSelector : ".nav a",
        itemSelector : "#post",
        behavior     : 'twitter',
        extraScrollPx: 50, 
        bufferPx     : 5000,
        loadingImg   : "http://static.tumblr.com/vaqsgdj/s5Pmgyueb/loading_ani.gif",
        loadingText  : "",
        loading      : "",
        donetext     : ""
    },
    
    function( newElements ) { 
        var $newElems = $( newElements ).css({ 
            opacity: 0 
        });
        $newElems.imagesLoaded(function(){
            $newElems.animate({ opacity: 1 });
            $content.masonry( "appended", $newElems, true );
        });
        $('.audio').each(function(){
            var audioID = $(this).attr("id");
            var $audioPost = $(this);
                $.ajax({
                url: 'http://{name}.tumblr.com/api/read/json?id=' + audioID,
                dataType: 'jsonp',
                timeout: 50000,
                success: function(data){
                $audioPost.find('#audio_player_'+audioID).replaceWith('<div style="background-color:#000000;">' + data.posts[0]['audio-player'].replace("audio_player.swf", "audio_player_black.swf") +'</div>');}
        });
    });
    });{/Block:IfInfiniteScroll}
    {/block:IndexPage}
                  
    $(document).load(function(){
        e.preventDefault();
        $wall.infinitescroll('retrieve');
    });  
    $('#load').click(function(){
      $(document).trigger('retrieve.infscr');
      return false;
    });
    });
   
    $(function() {      
      $(window).scroll(function() {
        if($(this).scrollTop() != 0) {
            $('#back_to_top').fadeIn();         
        } 
        else {
            $('#back_to_top').fadeOut();         
        }
      });
 
      $('#back_to_top #up a').click(function() {
    	$('body,html').animate({scrollTop:0},800);
	  });
    });{/block:IndexPage}

    $(document).ready(function() {
        $('#sidebar').stickyScroll({ container: '#container' });
    });
</script>

</body>
